{
    "api": {
        "address": "127.0.0.1",
        "port": 9991
    },
    "log": {
        "level": "trace",
        "output": "leaf.log"
    },
    "dns": {
        "bind": "192.168.50.3",
        "servers": [
            "8.8.8.8",
            "8.8.4.4"
        ],
        "hosts": {
            "example.com": [
                "192.168.0.1",
                "192.168.0.2"
            ]
        }
    },
    "inbounds": [
        {
            "protocol": "tun",
            "settings": {
                "name": "utun8",
                "address": "10.10.0.2",
                "netmask": "255.255.255.0",
                "gateway": "10.10.0.1",
                "mtu": 1500,
                "auto": true
            },
            "tag": "tun_in"
        },
        {
            "protocol": "socks",
            "settings": {
                "address": "1.2.3.4",
                "port": 1080
            },
            "tag": "socks_out"
        },
        {
            "protocol": "trojan",
            "address": "127.0.0.1",
            "port": 10086,
            "settings": {
                "password": "123456"
            }
        }
    ],
    "outbounds": [
        {
            "bind": "192.168.50.3",
            "protocol": "socks",
            "settings": {
                "address": "192.168.3.2",
                "port": 7890
            },
            "tag": "socks_out"
        },
        {
            "bind": "192.168.50.3",
            "protocol": "direct",
            "tag": "direct_out"
        }
    ],
    "router": {
        "domainResolve": true,
        "rules": [
            {
                "ip": [
                    "8.8.8.8",
                    "8.8.4.4"
                ],
                "target": "socks_out"
            },
            {
                "portRange": [
                    "22-22",
                    "1024-65535"
                ],
                "target": "direct_out"
            },
            {
                "domain": [
                    "www.google.com"
                ],
                "target": "socks_out"
            },
            {
                "domainSuffix": [
                    "google.com"
                ],
                "target": "socks_out"
            },
            {
                "domainKeyword": [
                    "google"
                ],
                "target": "socks_out"
            }
        ]
    }
}